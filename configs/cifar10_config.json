{
    "exp_name" : "cifar10_exp-009",
    "agent" : "Cifar10Agent",
    "inference_agent" : "Cifar10IAgent",

    "input_size" : [3,32,32],
    "mean" : [0.4914, 0.4822, 0.4465],
    "std" : [0.2023, 0.1994, 0.2010],
    
    "use_cuda" : true,
    "gpu_device" : 0,
    "seed" : 1,

    "batch_size" : 64,

    "learning_rate" : 0.1,
    "momentum" : 0.9,

    "l1_decay" : 1e-6,
    "l2_decay" : 0.005,

    "use_scheduler" : true,
    "scheduler" : {
        "name" : "OneCycleLR",
        "kwargs" : {
            "pct_cycle" : 0.3,
            "anneal_strategy" : "cos",
            "cycle_momentum" : true,
            "base_momentum" : 0.85,
            "max_momentum" : 0.95,
            "div_factor" : 10,
            "final_div_factor" : 1e4,
            "last_epoch" : -1
        }   
    },
    
    "save_checkpoint" : true,
    "load_checkpoint" : true,
    "checkpoint_file": "checkpoint.pth.tar",
    "model_stats_file" : "stats.json",

    "epochs" : 50,

    "num_workers" : 4,
    "pin_memory" : true,

    "interpret_image": true,
    "visualize_inline": true

}
